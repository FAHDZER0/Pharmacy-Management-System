<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="611.0" prefWidth="505.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.pharmacyv1.Suppliers.FindSupplier_PopUpController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Enter Supplier's Name">
         <font>
            <Font name="System Bold" size="19.0" />
         </font></Label>
        <TextField fx:id="txtSupplierName" layoutX="229.0" layoutY="13.0" prefHeight="30.0" prefWidth="177.0" promptText="Supplier Name">
         <font>
            <Font size="14.0" />
         </font></TextField>
        <Button layoutX="416.0" layoutY="13.0" mnemonicParsing="false" onAction="#findSupplier" prefHeight="30.0" prefWidth="86.0" text="Find">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
        <TableView fx:id="tblSupplier" layoutY="62.0" prefHeight="470.0" prefWidth="690.0">
            <columns>
                <TableColumn prefWidth="75.0" text="ID" />
                <TableColumn prefWidth="150.0" text="Name In English" />
            <TableColumn prefWidth="150.0" text="Name In Arabic" />
                <TableColumn prefWidth="150.0" text="Address" />
                <TableColumn prefWidth="150.0" text="Phone" />
            </columns>
        </TableView>
      <Button layoutX="71.0" layoutY="550.0" mnemonicParsing="false" onAction="#findSupplier" prefHeight="30.0" prefWidth="86.0" text="Confirm">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="373.0" layoutY="550.0" mnemonicParsing="false" onAction="#findSupplier" prefHeight="30.0" prefWidth="86.0" text="Cancel">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
    </children>


</AnchorPane>
